{
  "manifest_version": 2,
  "name": "Paranoia Password Manager",
  "version": "0.0.7",
  "default_locale": "en",
  "description": "__MSG_extension_description__",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/state_icons/unauthenticated.png"
    },
    "default_popup": "popup.html",
    "default_title": "Ppm Popup"
  },
  "options_ui": {
    "page": "settings.html",
    "browser_style": false,
    "open_in_tab": true
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{3f1a7524-8bc7-1b40-b2e7-9d026108db01}",
      "strict_min_version": "85.0"
    }
  },
  "icons": {
    "128": "images/paranoia_128.png",
    "32": "images/paranoia_32.png",
    "48": "images/paranoia_48.png",
    "64": "images/paranoia_64.png"
  },
  "omnibox": {
    "keyword": "PPM"
  },
  "content_security_policy": "default-src 'self'; connect-src http://*:* https://*:*; script-src 'self'; img-src 'self'; object-src 'self'; style-src 'self'",
  "permissions": [
    "tabs",
    "idle",
    "storage",
    "http://*/*",
    "https://*/*"
  ]
}
